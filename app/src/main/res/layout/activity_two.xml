<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D3C9C9"
    tools:context=".TwoActivity">



    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="36dp"
        android:textColor="#000000"
        android:text="ชื่อ-นามสกุล :"
        android:textSize="22dp"
        app:layout_constraintEnd_toStartOf="@+id/namelastEt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:textColor="#000000"
        android:text="อายุ :"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:text="เบอร์โทร :"
        android:textColor="#000000"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:textColor="#000000"
        android:text="อีเมล :"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:textColor="#000000"
        android:text="แผนก :"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="ชื่อเล่น :"
        android:textColor="#000000"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <Button
        android:id="@+id/backBtn"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="238dp"
        android:layout_height="65dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/green"
        android:backgroundTint="#FF5722"
        android:text="กลับ"
        android:textColor="#FFFFFF"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.467"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nicknameTv" />

    <Button
        android:id="@+id/closeBtn"
        android:layout_width="238dp"
        android:layout_height="65dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/green"
        android:backgroundTint="#000000"
        android:text="จบการทำงาน"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#FFFFFF"
        android:textSize="22dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/backBtn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/backBtn"
        app:layout_constraintTop_toBottomOf="@+id/backBtn"
        app:layout_constraintVertical_bias="0.517" />

    <TextView
        android:id="@+id/namelastTv"
        android:layout_width="233dp"
        android:layout_height="62dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="16dp"
        android:background="#4CAF50"
        android:text="namelast"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ageTv"
        android:layout_width="233dp"
        android:layout_height="62dp"
        android:layout_marginTop="28dp"
        android:background="#4CAF50"
        android:text="age"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="@+id/namelastTv"
        app:layout_constraintTop_toBottomOf="@+id/namelastTv" />

    <TextView
        android:id="@+id/phoneTv"
        android:layout_width="233dp"
        android:layout_height="62dp"
        android:layout_marginTop="28dp"
        android:background="#4CAF50"
        android:text="phone"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="@+id/ageTv"
        app:layout_constraintTop_toBottomOf="@+id/ageTv" />

    <TextView
        android:id="@+id/emailTv"
        android:layout_width="233dp"
        android:layout_height="62dp"
        android:layout_marginTop="28dp"
        android:background="#4CAF50"
        android:text="email"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="@+id/phoneTv"
        app:layout_constraintTop_toBottomOf="@+id/phoneTv" />

    <TextView
        android:id="@+id/departmentTv"
        android:layout_width="233dp"
        android:layout_height="62dp"
        android:layout_marginTop="28dp"
        android:background="#4CAF50"
        android:text="de"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="@+id/emailTv"
        app:layout_constraintTop_toBottomOf="@+id/emailTv" />

    <TextView
        android:id="@+id/nicknameTv"
        android:layout_width="233dp"
        android:layout_height="62dp"
        android:layout_marginTop="28dp"
        android:background="#4CAF50"
        android:text="nick"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="@+id/departmentTv"
        app:layout_constraintTop_toBottomOf="@+id/departmentTv" />

</androidx.constraintlayout.widget.ConstraintLayout>